<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Cron that process the mass invoice and bills -->
        <record id="ir_cron_mass_account_move" model="ir.cron">
            <field name="name">Mass Invoicing/Billing: Process queue</field>
            <field name="model_id" ref="model_mass_account_move"/>
            <field name="state">code</field>
            <field name="code">model._process_mass_account_move_queue()</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="interval_number">60</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>
    </data>
</odoo>
